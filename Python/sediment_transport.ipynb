    "def calculate_omega_for_season(Hb, V):\n",
    "    OMEGA = []\n",
    "    for j in range(Hb.shape[0]):\n",
    "        omega = Hb[j, :]**2 * V[j, :]\n",
    "        OMEGA.append(omega)\n",
    "    return np.array(OMEGA)"
   ]
  },
