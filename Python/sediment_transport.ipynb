   "source": [
    "def calculate_transport_for_season(OMEGA):\n",
    "    TRANSPORT = []\n",
    "    for j in range(OMEGA.shape[0]):\n",
    "        omega = OMEGA[j, :]\n",
    "        transport = np.array([calculate_transport_value(o) for o in omega])\n",
    "        TRANSPORT.append(transport)\n",
    "    return np.array(TRANSPORT)"
   ]
  },
