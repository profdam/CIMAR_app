    "            Thetab[i], Hb[i], hb[i], R[i], S[i], V[i] = adapted_Larson2010(tm_f[i], hs_f[i], dir_f[i])\n",
    "        \n",
    "        Thetab_f.append(Thetab)\n",
    "        Hb_f.append(Hb)\n",
    "        hb_f.append(hb)\n",
    "        R_f.append(R)\n",
    "        S_f.append(S)\n",
    "        V_f.append(V)\n",
    "    \n",
    "    return np.array(Thetab_f), np.array(Hb_f), np.array(hb_f), np.array(R_f), np.array(S_f), np.array(V_f)\n"
   ]
